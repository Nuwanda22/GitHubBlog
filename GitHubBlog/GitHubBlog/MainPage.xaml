<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:GitHubBlog"
             x:Class="GitHubBlog.MainPage">

  <ContentPage.Content>
    <StackLayout>
      <Entry x:Name="TitleEntry" Placeholder="Title"/>
      <Editor x:Name="ContentEditor"/>
      <Button Text="Send" Clicked="OnClicked"/>
    </StackLayout>
  </ContentPage.Content>

</ContentPage>